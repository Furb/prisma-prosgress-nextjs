datasource db {
  provider          = "postgresql"
  url               = env("DATABASE_URL")
  directUrl         = env("DIRECT_URL")
}

generator client {
  provider = "prisma-client-js"
}


model Booking {
  id                 Int      @id @default(autoincrement())
  subject            String
  startTime          DateTime
  endTime            DateTime
  description        String?
}

